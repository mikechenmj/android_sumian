<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--itemTab-->
    <declare-styleable name="NavigationItem">
        <attr name="tab_icon" format="reference" />
        <attr name="tab_text" format="string|reference" />
        <attr name="tab_text_size" format="dimension|reference" />
    </declare-styleable>

    <!--divider view-->
    <declare-styleable name="SettingDividerView">
        <attr name="type_icon" format="reference" />
        <attr name="type_desc" format="string|reference" />
        <attr name="type_desc_text_color" format="color|reference" />
        <attr name="type_desc_text_size" format="dimension|reference" />
        <attr name="dot_color" format="color|reference" />
        <attr name="type_content" format="string|reference" />
        <attr name="type_content_text_color" format="color|reference" />
        <attr name="type_content_text_size" format="dimension|reference" />
        <attr name="divider_line_color" format="color|reference" />
        <attr name="divider_line_size" format="dimension|reference" />
        <attr name="divider_line_visible" format="enum">
            <enum name="gone" value="0x00000008" />
            <enum name="visible" value="0x00000000" />
            <enum name="invisible" value="0x00000004" />
        </attr>
        <attr name="divider_more_visible" format="enum">
            <enum name="gone" value="0x00000008" />
            <enum name="visible" value="0x00000000" />
            <enum name="invisible" value="0x00000004" />
        </attr>
        <attr name="show_switch" format="boolean" />
    </declare-styleable>

    <!--title bar-->
    <declare-styleable name="TitleBar">
        <attr name="text" format="string|reference" />
        <attr name="text_size" format="dimension|reference" />
        <attr name="show_back" format="boolean|reference" />
        <attr name="show_spanner" format="boolean|reference" />
        <attr name="menu_icon" format="reference" />
        <attr name="menu_text" format="string|reference" />
    </declare-styleable>

    <!--pay item view-->
    <declare-styleable name="PayItemView">
        <attr name="icon" format="reference" />
        <attr name="desc" format="string|reference" />
        <attr name="is_select" format="boolean|reference" />
    </declare-styleable>

    <!--BubbleImageView-->
    <declare-styleable name="BubbleImageView">
        <attr name="chat_image_mask" format="reference" />
    </declare-styleable>


    <declare-styleable name="PaletteImageView">
        <attr name="paletteRadius" format="dimension" />
        <attr name="paletteSrc" format="reference" />
        <attr name="palettePadding" format="dimension" />
        <attr name="paletteOffsetX" format="dimension" />
        <attr name="paletteOffsetY" format="dimension" />
        <attr name="paletteShadowRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ShapeImageView">
        <attr name="riv_border_color" format="color" />
        <attr name="riv_border_width" format="dimension" />
        <attr name="riv_corner_radius" format="dimension" />
        <attr name="riv_is_circle" format="boolean" />
        <attr name="riv_pressed_border_color" format="color" />
        <attr name="riv_pressed_border_width" format="dimension" />
        <attr name="riv_pressed_mask_color" format="color" />
        <attr name="riv_pressed_mode_enabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CircleProgressView">
        <attr name="cpv_progressNormalColor" format="color" />
        <attr name="cpv_progressReachColor" format="color" />
        <attr name="cpv_progressTextColor" format="color" />
        <attr name="cpv_progressTextSize" format="dimension" />
        <attr name="cpv_progressTextOffset" format="dimension" />
        <attr name="cpv_progressNormalSize" format="dimension" />
        <attr name="cpv_progressReachSize" format="dimension" />
        <attr name="cpv_radius" format="dimension" />
        <attr name="cpv_progressTextVisible" format="boolean" />
        <attr name="cpv_progressStartArc" format="integer" />
        <attr name="cpv_progressTextSkewX" format="dimension" />
        <attr name="cpv_progressTextPrefix" format="string" />
        <attr name="cpv_progressTextSuffix" format="string" />
        <attr name="cpv_innerBackgroundColor" format="color" />
        <attr name="cpv_progressStyle" format="enum">
            <enum name="Normal" value="0" />
            <enum name="FillInner" value="1" />
            <enum name="FillInnerArc" value="2" />
        </attr>
        <attr name="cpv_innerProgressColor" format="color" />
        <attr name="cpv_innerPadding" format="dimension" />
        <attr name="cpv_outerColor" format="color" />
        <attr name="cpv_outerSize" format="dimension" />
        <attr name="cpv_reachCapRound" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DoctorServiceItemView">
        <attr name="dsiv_title" format="string" />
        <attr name="dsiv_desc" format="string" />
        <attr name="dsiv_show_price" format="boolean" />
        <attr name="dsiv_price" format="float" />
        <attr name="dsiv_image" format="reference" />
    </declare-styleable>

    <declare-styleable name="ColorfulProgressView">
        <attr name="cpv_ring_width" format="float|dimension" />
        <attr name="cpv_ring_bg_color" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="ProgressView">
        <attr name="pv_ring_width" format="float|dimension" />
        <attr name="pv_ring_bg_color" format="color|reference" />
        <attr name="pv_progress_color" format="color|reference" />
        <attr name="pv_progress" format="integer|reference" />
        <attr name="pv_max_progress" format="integer|reference" />
    </declare-styleable>

    <declare-styleable name="TitleView">
        <attr name="title_text" format="string|reference" />
        <attr name="content_text" format="string|reference" />
        <attr name="tv_show_right_arrow" format="boolean|reference" />
    </declare-styleable>

    <declare-styleable name="WithLineProgressView">
        <attr name="wlpv_show_line" format="boolean|reference" />
    </declare-styleable>
</resources>