<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.sumian.sd.record.widget.ColorfulProgressView
        android:id="@+id/progress_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        app:cpv_ring_width="@dimen/space_5" />

    <com.sumian.hw.report.widget.text.CountSleepDurationTextView
        android:id="@+id/count_duration_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:textColor="@color/b3_color_day"
        app:csdtv_default_drawable="@drawable/bg_text_b3"
        app:csdtv_number_text_size="@dimen/font_21"
        app:csdtv_unit_text_size="@dimen/font_13"
        tools:text="123" />

</FrameLayout>